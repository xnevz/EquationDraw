<UserControl x:Class="EquationDraw.OperatorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EquationDraw"
             mc:Ignorable="d" Width="15" Height="15"
             Name="control"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Path Stretch="Fill" Stroke="White" StrokeThickness="2" Data="{Binding SymbolData, ElementName=control, FallbackValue='M0,1 L1, 0 M0, 0 L1, 1'}" />
    </Grid>
</UserControl>
